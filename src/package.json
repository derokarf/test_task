    "scripts": {
    "build": "npx tsc -p tsconfig.build.json --sourceMap false --declaration false && npx tsc-alias -p tsconfig.json",
    "start:dev": "set -a && source ../.env && node --max-old-space-size=2048 --inspect=0.0.0.0:5555 --require ts-node/register --require tsconfig-paths/register main.ts",
    "unit-tests": "npx jest ./ --ci --config=\"jest.unit.config.js\"",
    "integrational-tests": "npx jest ./ --ci --config=\"jest.integrational.config.js\" --runInBand --forceExit",
    "migration-run": "set -a && source ../.env && SKIP_MIGRATIONS=0 && npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d typeorm.ts migration:run",
    "migration-revert": "set -a && source ../.env && SKIP_MIGRATIONS=0 && npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d typeorm.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-express": "^10.4.12",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/typeorm": "^10.0.2",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1"
},
"devDependencies": {
    "@nestjs/testing": "^10.2.6",
